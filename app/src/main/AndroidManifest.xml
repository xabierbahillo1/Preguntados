<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.das.preguntados">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        android:usesCleartextTraffic="true">

        <activity android:name=".Activitys.RankingActivityAlt"></activity>
        <activity android:name=".Activitys.RankingActivity" />
        <activity android:name=".Activitys.DueloActivity" />
        <activity android:name=".Activitys.RoomActivity" />
        <activity android:name=".Activitys.GameSelectorActivity" />
        <activity android:name=".Activitys.EditProfileActivity" />
        <activity android:name=".Activitys.ProfileActivity" />
        <activity android:name=".Activitys.GameActivity" />
        <activity
            android:name=".Activitys.SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activitys.RegistroActivity" />
        <activity android:name=".Activitys.MainMenuActivity" />
        <activity android:name=".Activitys.LoginActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>